#include<iostream>
using namespace std;
int main()
{
    int n,temp,dig,rev=0;
    cin>>n;
    temp=n;
    while(n!=0)
    {
        dig=n%10;
        rev=(rev*10)+dig;
        n=n/10;
    }
    if(temp==rev)
    {
    cout<<"Same"<<endl;
    }
    else
    {
        cout<<"Not Same"<<endl;
    }   
}
