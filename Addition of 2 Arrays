#include<iostream>
using namespace std;
int main()
{
    int n,i;
    cin>>n;
    int a[n],b[n],c[i];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<n;i++)
    {
        cin>>b[i];
    }
    for(int i=0;i<n;i++)
    {
        c[i]=a[i]+b[i];
        cout<<c[i]<<endl;
    }
}
