#include <iostream>
using namespace std;
int add(int c,int d)
{
    cout<<c+d<<endl;
    return 0;
}
int sub(int c,int d)
{
    cout<<c-d<<endl;
    return 0;
}
int mul(int c,int d)
{
    cout<<c*d<<endl;
    return 0;
}
int division(int c,int d)
{
    cout<<c/d<<endl;
    return 0;
}
int mod(int c,int d)
{
    cout<<c%d<<endl;
     return 0;
}
int main()
{
    int a,b;
    cin>>a>>b;
    add(a,b);
    sub(a,b);
    mul(a,b);
    division(a,b);
    mod(a,b);
}
