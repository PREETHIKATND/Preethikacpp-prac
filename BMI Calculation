#include<iostream>
using namespace std;
int main()
{
    float a,b,c;
    cin>>a>>b;
    c=(a/(b*b))*10000;
    if(c>=18.5 && c<=25)
    {
        cout<<"You are normal.Go walking daily and maintain it"<<endl;
    }
    else if(c<18.5)
    {
        cout<<"You are underweight.Have an apple daily"<<endl;
    }
    else
    {
        cout<<"You are obese.Go to doctor"<<endl;
    }
}
